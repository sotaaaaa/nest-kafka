{
  "name": "@sotaaaaa/nest-kafka",
  "version": "1.0.4",
  "private": false,
  "description": "NestJS - Kafka module",
  "keywords": [
    "nest",
    "core",
    "nodejs",
    "sotaaaaa",
    "kafka",
    "nestjs"
  ],
  "homepage": "https://github.com/sotaaaaa/nest-kafka#readme",
  "bugs": {
    "url": "https://github.com/sotaaaaa/nest-kafka/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotaaaaa/nest-kafka.git"
  },
  "contributors": [
    "Sotaaaaa <dungla2708@gmail.com>"
  ],
  "author": "Sota",
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "deploy": "sh ./scripts/publish.sh",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.2",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.2",
    "@nestjs/microservices": "^8.4.2",
    "@nestjs/platform-express": "^8.4.2",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
